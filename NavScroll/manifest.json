{
    "manifest_version": 3,
    "name": "NavScroll",
    "version": "1.1",
    "description": "在网页中添加一个导航器",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["pagemap.min.js", "contentScript.js"],
        "run_at": "document_idle"
    }],
    "action": {
        "default_icon": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}

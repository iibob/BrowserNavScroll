{
    "manifest_version": 3,
    "name": "PageMap",
    "version": "1.0",
    "description": "在所有网页中添加一个迷你地图",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["pagemap.min.js", "contentScript.js"],
        "css": ["styles.css"]
    }]
}
